@Article{Metzner2012,
  Title                    = {{Functional renormalization group approach to correlated fermion systems}},
  Author                   = {{Metzner}, W. and {Salmhofer}, M. and {Honerkamp}, C. and {Meden}, V. and {Sch{\"o}nhammer}, K.},
