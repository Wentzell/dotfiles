@Article{Schuster1997,
  Title                    = {{Phase measurement in a quantum dot via a double-slit interference experiment}},
  Author                   = {{Schuster}, R. and {Buks}, E. and {Heiblum}, M. and {Mahalu}, D. and {Umansky}, V. and {Shtrikman}, H.},
