@Article{Karrasch2007,
  Title                    = {{Phase lapses in transmission through interacting two-level quantum dots}},
  Author                   = {{Karrasch}, C. and {Hecht}, T. and {Weichselbaum}, A. and {von Delft}, J. and {Oreg}, Y. and {Meden}, V.},
