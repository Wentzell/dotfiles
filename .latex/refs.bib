}

@Article{Karrasch2008a,
  Title                    = {{A finite-frequency functional renormalization group approach to the single impurity Anderson model}},
  Author                   = {{Karrasch}, C. and {Hedden}, R. and {Peters}, R. and {Pruschke}, T. and {Sch{\"o}nhammer}, K. and {Meden}, V.},
