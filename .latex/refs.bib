@Article{Durganandini2006,
  Title                    = {{Two-impurity Anderson model for the transport through two quantum dots coupled via a quantum wire}},
  Author                   = {{Durganandini}, P.},
  Journal                  = {Phys. Rev. B},
  Year                     = {2006},

  Month                    = oct,
  Number                   = {15},
  Pages                    = {155309},
  Volume                   = {74},

  Abstract                 = {We study the problem of transport through two quantum dots coupled via a finite-length quantum wire. In the limit where each dot acts essentially as a localized Kondo impurity, the geometry allows us to study various questions in the many-impurity problem like the interplay of Kondo and RKKY interactions, the average interimpurity distance beyond which the single-impurity model becomes valid, etc. We model the system in terms of the two-impurity Anderson model and study the transport behavior using a perturbative (in the Coulomb interaction U ) approach, restricting ourselves to the ``symmetric'' case. It can be seen that (antiferromagnetic) ferromagnetic correlations develop between the impurities for (``odd'') ``even'' distances between the impurities. We analyze the antiferromagnetic case in detail and show that the linear conductance for the coupled dot system has a nonmonotonic temperature dependence in contrast to the monotonic temperature dependence for the single-dot system. A temperature scale emerges in the problem---the coherence temperature---below which the two impurities develop strong correlations. The emergence of this scale is also reflected in the behavior of the impurity Green functions. In particular, the zero-temperature diagonal impurity density of states shows a splitting of the Kondo resonance with the splitting width being of the same order as the coherence temperature. The coherence temperature decreases with an increase in the distance between the impurities and increases with an increase in U . Finally, we also study transport in the ``side'' geometry where the dots are side coupled to a long wire at a finite distance and show that essentially the same physics is observed for the antiferromagnetic case as in the ``chain'' geometry.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/2006PhRvB..74o5309D},
