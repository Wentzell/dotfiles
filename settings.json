},
{
"before": ["<Leader>", "C", "t", "l"],
"commands": [":CommTLine"]
},
{
"before": ["<Leader>", "d", "c"],
"commands": [":DoxComm"]
}
],
"vim.normalModeKeyBindingsNonRecursive": [
{
"before": ["<leader>", "<leader>", "p"],
