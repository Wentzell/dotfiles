}
],
"vim.visualModeKeyBindingsNonRecursive": [
{
"before": [";", ";"],
"after": ["<Esc>"]
},
{
"before": ["<leader>", "<leader>", "y"],
"after": ["\"", "+", "y"]
},
{
"before": ["<leader>", "<leader>", "p"],
"after": ["\"", "+", "p"]
},
{
"before": [";", "j"],
"after": ["<C-D>"]
},
{
"before": [";", "k"],
"after": ["<C-U>"]
},
{
"before": ["<C-r>"],
"after": ["\"", "h", "y", ":", "%", "s", "/", "<C-r>", "h", "/", "/", "g", "c", "<left>", "<left>", "<left>"]
}
],
"vim.insertModeKeyBindingsNonRecursive": [
{
"before": [";", ";"],
"after": ["<Esc>"]
},
{
"before": ["{"],
"after": ["{", "}", "<Left>"]
},
{
"before": ["{", "<cr>"],
